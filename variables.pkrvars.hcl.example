# Example variables file
# Copy this to variables.auto.pkrvars.hcl and customize

# Hetzner Cloud API token
# hcloud_token = "your-token-here"

# VyOS version identifier
# vyos_version = "1.4-rolling-latest"

# Server location (nbg1, fsn1, hel1, ash, hil)
# server_location = "nbg1"

# Server type for building (cx11, cx21, cx31, etc.)
# server_type = "cx11"

# Custom VyOS ISO URL (optional)
# vyos_iso_url = "https://example.com/vyos.iso"
